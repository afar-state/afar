{
  "name": "fetch-retry-handler",
  "$schema": "https://www.unpkg.com/nx/schemas/project-schema.json",
  "sourceRoot": "libs/fetch-retry-handler/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/fetch-retry-handler",
        "main": "libs/fetch-retry-handler/src/index.ts",
        "tsConfig": "libs/fetch-retry-handler/tsconfig.lib.json",
        "project": "libs/fetch-retry-handler/package.json",
        "updateBuildableProjectDepsInPackageJson": true,
        "format": [
          "esm"
        ],
        "assets": []
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs fetch-retry-handler {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/fetch-retry-handler/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/fetch-retry-handler/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
